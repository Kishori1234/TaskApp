<h1>To Do List</h1>
<body>
<table border=4 cellspacing=4 cellpadding=5>
<tr>
	<td>Task No</td>
	<td>Task Name</td>
	<td>Status</td>
</tr>	
<% @task.each do |t|  %>

	<tr>
		<td>
	<%= t.taskId %>
</td>
<td>
	<%=t.taskName %>
	</td>
	<td>
		<%=t.status %>
	</td>
</tr>
<%end%>
</table>
<div>
<form name="task">
	<%= link_to "insert", new_task_path , :id=>"insert_id" %>
	<input type="submit" value="delete">
	<input type="submit" value="update">
</form>
</div>
</body>